{
  "expo": {
    "updates": {
      "enabled": true,
      "url": "https://u.expo.dev/84e4491f-58ec-40d3-b141-bd49c10e0b44",
      "runtimeVersion": "1.0.0"
    },
    "name": "cuna_app",
    "slug": "cuna_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "We need your permission to use the camera for real-time detection.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.erickdominguez.cunaapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "CAMERA",
        "CAMERA",
        "CAMERA"
      ],
      "package": "com.erickdominguezm.cuna_app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "84e4491f-58ec-40d3-b141-bd49c10e0b44"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
